    "jest-runner-eslint": "^2.1.2",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "react-test-renderer": "^18.2.0",
